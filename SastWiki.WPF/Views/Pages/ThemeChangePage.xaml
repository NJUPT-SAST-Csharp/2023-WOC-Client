<Page x:Class="SastWiki.WPF.Views.Pages.ThemeChangePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SastWiki.WPF.Views.Pages"
      xmlns:themeswitch="https://jamesnet.dev/xaml/presentation/themeswitch"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ThemeChangePage">

    <Grid Background="{DynamicResource Gray}">
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <StackPanel Grid.Row="1" Orientation="Vertical">
            <themeswitch:ThemeSwitch Click="ThemeSwitch_Checked"  IsChecked="True"/>

        </StackPanel>
    </Grid>
</Page>
