<Page
    x:Class="SastWiki.WPF.Views.Pages.SearchResultPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:SastWiki.WPF.Views.Pages"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:viewmodels="clr-namespace:SastWiki.WPF.ViewModels"
    Title="SearchResultPage"
    d:DataContext="{d:DesignInstance Type=viewmodels:SearchResultVM}"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <StackPanel>
        <TextBlock Margin="60,30,60,0" FontSize="40">Search Result</TextBlock>
        <TextBlock
            Margin="60,10,60,0"
            FontSize="20"
            Text="{Binding SearchText, StringFormat='Search For {0}'}" />
    </StackPanel>

</Page>
