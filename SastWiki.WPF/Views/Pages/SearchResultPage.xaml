<Page
    x:Class="SastWiki.WPF.Views.Pages.SearchResultPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:SastWiki.WPF.Views.Pages"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:viewmodels="clr-namespace:SastWiki.WPF.ViewModels"
    Title="SearchResultPage"
    d:DataContext="{d:DesignInstance Type=viewmodels:SearchResultVM}"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <StackPanel>
        <TextBlock Text="Search Result Page" />
        <TextBlock Text="{Binding InquiryText}" />
    </StackPanel>
</Page>
